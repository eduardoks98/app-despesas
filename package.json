    "type-check": "tsc --noEmit",
    "prebuild": "npx expo prebuild --clean",
    "clean": "rm -rf node_modules && npm install",
    "backup": "node scripts/backup.js",
    "deploy": "npm run build:android"
  },
  "dependencies": {
