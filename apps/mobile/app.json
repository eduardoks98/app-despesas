{
  "expo": {
    "name": "App Despesas",
    "slug": "controle-financeiro",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "../../assets/icons/app-icon.png",
    "license": "AGPL-3.0",
    "githubUrl": "https://github.com/eduardoks98/app-despesas",
    "privacy": "public",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#8B5CF6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.appdespesas.app",
      "icon": "../../assets/icons/app-icon.png"
    },
    "android": {
      "package": "com.appdespesas.app",
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "../../assets/icons/app-icon.png",
        "backgroundColor": "#8B5CF6"
      },
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "VIBRATE"
      ],
      "navigationBar": {
        "backgroundColor": "#000000",
        "barStyle": "light-content"
      },
      "statusBar": {
        "backgroundColor": "#8B5CF6",
        "barStyle": "light-content"
      }
    },
    "web": {
      "favicon": "../../assets/icons/web/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "97dd8346-6a0a-469d-b8db-917e7045da29"
      },
      "license": {
        "type": "AGPL-3.0",
        "url": "https://github.com/eduardoks98/app-despesas/blob/main/LICENSE"
      }
    },
    "plugins": [
      "expo-sqlite",
      "expo-navigation-bar"
    ]
  }
}